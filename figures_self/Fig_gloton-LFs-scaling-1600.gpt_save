set term epslatex size 7.2,5 color colortext 20 standalone header "\\newcommand{\\LF}[1]{$\\scriptstyle{l_\\mathrm{frag}\\,=\\,#1}$}"
set output "Fig_gloton-LFs-scaling-1600.tex"


TOP=0.98
BOTTOM=0.12
LEFT=0.1
RIGHT=0.998

set tmargin at screen TOP
set bmargin at screen BOTTOM
set lmargin at screen LEFT
set rmargin at screen RIGHT

set xrange [1-1.58:1-0.42]
set yrange [0:1]
set ylabel "success probability" offset 3.5,0
sw(x)=1/(x*x*1)
set label  "$N_\\mathrm{frag}=1600$" at 0.2,0.5
set key top left Left reverse samplen 1
set format x "\\small%g"
set format y "\\small%g"
set xtics ("\\small $-0.5$" -0.5,0,0.5) offset 0,0.5
set ytics offset 0.9,0

#set xrange [0.95:1.11]
#set yrange [0.2:0.7]

a=4.9

set xlabel sprintf("$\\langle x \\rangle-%g/l_\\mathrm{frag}$",a) offset 0,1

